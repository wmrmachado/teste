package projeto_fila;

public class ProjetoFila {
	private int inicio;
	private int fim;
	private int vetor[];
	private int total;
	
	public ProjetoFila() {
		vetor = new int[10];
		inicio = -1;
		fim = -1;
	}
	
	public boolean vazio() {
		return (total == 0);
	}
	
	public boolean cehio() {
		return (total == 9);
	}
	
	public void inserir(int valor) {
		if(!cheio()) {
			fim++;
			vetor[fim] = valor;
			total++;
			
		}
	}

}
