package teste;

import javax.swing.JOptionPane;

import modelo.Aluno;

public class Teste {

	public static void main(String[] args) {
		
		Aluno objeto = new Aluno();
		objeto.adicionarNome(JOptionPane.showInputDialog("Digite o nome"));
		
		System.out.println("O nome é: " + objeto.retornarNome());
		System.out.println("Até logo...");
		
		
	}

}
